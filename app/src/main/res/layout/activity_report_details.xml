<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bgColor1"
    android:orientation="vertical"
    tools:context=".ui.activity.room.ReportDetailsActivity">

    <include layout="@layout/common_top1" />

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_150"
                    android:background="@color/bgColor11"
                    android:orientation="vertical">


                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <TextView
                            android:id="@+id/name"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:layout_centerInParent="true"
                            android:layout_marginLeft="@dimen/dp_32"
                            android:layout_marginTop="@dimen/dp_32"
                            android:text="吴青禾"
                            android:textColor="@color/textColor13"
                            android:textSize="@dimen/font_16" />

                        <ImageView
                            android:id="@+id/sexIv"
                            android:layout_width="@dimen/dp_19"
                            android:layout_height="@dimen/dp_20"
                            android:layout_centerInParent="true"
                            android:layout_marginLeft="@dimen/dp_25"
                            android:layout_toRightOf="@+id/name"
                            android:src="@mipmap/icon_man" />


                    </RelativeLayout>


                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/len_32"
                        android:layout_marginRight="@dimen/len_32">

                        <TextView
                            android:id="@+id/age_title"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:layout_centerInParent="true"
                            android:layout_marginTop="@dimen/dp_10"
                            android:text="@string/age_title1"
                            android:textColor="@color/textColor13"
                            android:textSize="@dimen/font_14" />


                        <TextView
                            android:id="@+id/ageTv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true"
                            android:layout_marginTop="@dimen/dp_10"
                            android:layout_toRightOf="@+id/age_title"
                            android:textColor="@color/textColor13"
                            android:textSize="@dimen/font_14" />


                        <TextView
                            android:id="@+id/marital_statusTitle"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true"
                            android:layout_marginTop="@dimen/dp_10"
                            android:layout_toLeftOf="@+id/maritalStatusTv"
                            android:text="@string/marital_status"
                            android:textColor="@color/textColor13"
                            android:textSize="@dimen/font_14" />

                        <TextView
                            android:id="@+id/maritalStatusTv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerInParent="true"
                            android:layout_marginTop="@dimen/dp_10"
                            android:text=""
                            android:textColor="@color/textColor13"
                            android:textSize="@dimen/font_14" />


                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/len_32"
                        android:layout_marginRight="@dimen/len_32">

                        <TextView
                            android:id="@+id/birthday_title"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:layout_centerInParent="true"
                            android:layout_marginTop="@dimen/dp_8"
                            android:text="@string/birthday_title1"
                            android:textColor="@color/textColor13"
                            android:textSize="@dimen/font_14" />

                        <TextView
                            android:id="@+id/birthdayTv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true"
                            android:layout_marginTop="@dimen/dp_8"
                            android:layout_toRightOf="@+id/birthday_title"
                            android:text=""
                            android:textColor="@color/textColor13"
                            android:textSize="@dimen/font_14" />


                        <TextView
                            android:id="@+id/weightTitle"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true"
                            android:layout_marginTop="@dimen/dp_10"
                            android:layout_toLeftOf="@+id/weightTv"
                            android:text="@string/label_weight"
                            android:textColor="@color/textColor13"
                            android:textSize="@dimen/font_14" />

                        <TextView
                            android:id="@+id/weightTv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerInParent="true"
                            android:layout_marginTop="@dimen/dp_10"
                            android:text=""
                            android:textColor="@color/textColor13"
                            android:textSize="@dimen/font_14" />
                    </RelativeLayout>

                </LinearLayout>

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dp_12"
                    android:layout_marginTop="@dimen/len_10"
                    android:layout_marginRight="@dimen/dp_12"
                    android:background="@color/bgColor10">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">

                            <TextView
                                android:id="@+id/diagnosisNoTitle"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/dp_18"
                                android:layout_marginTop="@dimen/dp_18"
                                android:text="@string/diagnosis_no"
                                android:textColor="@color/textColor11"
                                android:textSize="@dimen/font_14" />


                            <TextView
                                android:id="@+id/diagnosisNoTv"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/dp_18"
                                android:layout_marginTop="@dimen/dp_18"
                                android:layout_marginRight="@dimen/dp_18"
                                android:layout_toRightOf="@+id/diagnosisNoTitle"
                                android:text=""
                                android:textColor="@color/textColor7"
                                android:textSize="@dimen/font_14" />

                        </RelativeLayout>


                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">

                            <TextView
                                android:id="@+id/diagnosisIdTitle"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/dp_18"
                                android:layout_marginTop="@dimen/dp_18"
                                android:text="@string/diagnosis_id"
                                android:textColor="@color/textColor11"
                                android:textSize="@dimen/font_14" />

                            <TextView
                                android:id="@+id/diagnosisIdTv"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/dp_18"
                                android:layout_marginTop="@dimen/dp_18"
                                android:layout_marginRight="@dimen/dp_18"
                                android:layout_toRightOf="@+id/diagnosisIdTitle"
                                android:text=""
                                android:textColor="@color/textColor7"
                                android:textSize="@dimen/font_14" />

                        </RelativeLayout>


                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">

                            <TextView
                                android:id="@+id/sectionOfficeNameTitle"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/dp_18"
                                android:layout_marginTop="@dimen/dp_18"
                                android:text="@string/section_office_name"
                                android:textColor="@color/textColor11"
                                android:textSize="@dimen/font_14" />

                            <TextView
                                android:id="@+id/sectionOfficeNameTv"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/dp_18"
                                android:layout_marginTop="@dimen/dp_18"
                                android:layout_marginRight="@dimen/dp_18"
                                android:layout_toRightOf="@+id/sectionOfficeNameTitle"
                                android:text=""
                                android:textColor="@color/textColor7"
                                android:textSize="@dimen/font_14" />

                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">

                            <TextView
                                android:id="@+id/doctorNameTitle"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/dp_18"
                                android:layout_marginTop="@dimen/dp_18"
                                android:text="@string/doctor_name"
                                android:textColor="@color/textColor11"
                                android:textSize="@dimen/font_14" />

                            <TextView
                                android:id="@+id/doctorNameTv"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/dp_18"
                                android:layout_marginTop="@dimen/dp_18"
                                android:layout_marginRight="@dimen/dp_18"
                                android:layout_toRightOf="@+id/doctorNameTitle"
                                android:text=""
                                android:textColor="@color/textColor7"
                                android:textSize="@dimen/font_14" />

                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">

                            <TextView
                                android:id="@+id/diagnosisStartTimeTitle"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/dp_18"
                                android:layout_marginTop="@dimen/dp_18"
                                android:layout_marginBottom="@dimen/dp_18"
                                android:text="@string/diagnosis_start_time"
                                android:textColor="@color/textColor11"
                                android:textSize="@dimen/font_14" />

                            <TextView
                                android:id="@+id/diagnosisStartTimeTv"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/dp_18"
                                android:layout_marginTop="@dimen/dp_18"
                                android:layout_marginRight="@dimen/dp_18"
                                android:layout_toRightOf="@+id/diagnosisStartTimeTitle"
                                android:text=""
                                android:textColor="@color/textColor7"
                                android:textSize="@dimen/font_14" />

                        </RelativeLayout>

                    </LinearLayout>

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dp_12"
                    android:layout_marginTop="@dimen/dp_18"
                    android:layout_marginRight="@dimen/dp_12"
                    android:background="@color/bgColor10">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">

                            <TextView
                                android:id="@+id/mainAppealTitle"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/dp_18"
                                android:layout_marginTop="@dimen/dp_18"
                                android:text="@string/main_appeal"
                                android:textColor="@color/textColor11"
                                android:textSize="@dimen/font_14" />


                            <TextView
                                android:id="@+id/mainAppealTv"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/dp_18"
                                android:layout_marginTop="@dimen/dp_18"
                                android:layout_marginRight="@dimen/dp_18"
                                android:layout_toRightOf="@+id/mainAppealTitle"
                                android:text=""
                                android:textColor="@color/textColor7"
                                android:textSize="@dimen/font_14" />

                        </RelativeLayout>


                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">

                            <TextView
                                android:id="@+id/currentMedicalHistorylTitle"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/dp_18"
                                android:layout_marginTop="@dimen/dp_18"
                                android:text="@string/current_medical_historyl"
                                android:textColor="@color/textColor11"
                                android:textSize="@dimen/font_14" />

                            <TextView
                                android:id="@+id/currentMedicalHistorylTv"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/dp_18"
                                android:layout_marginTop="@dimen/dp_18"
                                android:layout_marginRight="@dimen/dp_18"
                                android:layout_toRightOf="@+id/currentMedicalHistorylTitle"
                                android:text=""
                                android:textColor="@color/textColor7"
                                android:textSize="@dimen/font_14" />

                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">

                            <TextView
                                android:id="@+id/pastHistoryTitle"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/dp_18"
                                android:layout_marginTop="@dimen/dp_18"
                                android:text="@string/past_history"
                                android:textColor="@color/textColor11"
                                android:textSize="@dimen/font_14" />


                            <TextView
                                android:id="@+id/pastHistorylTv"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/dp_18"
                                android:layout_marginTop="@dimen/dp_18"
                                android:layout_marginRight="@dimen/dp_18"
                                android:layout_toRightOf="@+id/pastHistoryTitle"
                                android:text=""
                                android:textColor="@color/textColor7"
                                android:textSize="@dimen/font_14" />

                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">

                            <TextView
                                android:id="@+id/allergiesTitle"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/dp_18"
                                android:layout_marginTop="@dimen/dp_18"
                                android:text="@string/allergies"
                                android:textColor="@color/textColor11"
                                android:textSize="@dimen/font_14" />


                            <TextView
                                android:id="@+id/allergiesTv"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/dp_18"
                                android:layout_marginTop="@dimen/dp_18"
                                android:layout_marginRight="@dimen/dp_18"
                                android:layout_toRightOf="@+id/allergiesTitle"
                                android:text=""
                                android:textColor="@color/textColor7"
                                android:textSize="@dimen/font_14" />

                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">

                            <TextView
                                android:id="@+id/familyHistoryTitle"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/dp_18"
                                android:layout_marginTop="@dimen/dp_18"
                                android:layout_marginBottom="@dimen/dp_18"
                                android:text="@string/family_history"
                                android:textColor="@color/textColor11"
                                android:textSize="@dimen/font_14" />


                            <TextView
                                android:id="@+id/familyHistoryTv"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/dp_18"
                                android:layout_marginTop="@dimen/dp_18"

                                android:layout_marginRight="@dimen/dp_18"
                                android:layout_toRightOf="@+id/familyHistoryTitle"
                                android:text=""
                                android:textColor="@color/textColor7"
                                android:textSize="@dimen/font_14" />

                        </RelativeLayout>

                    </LinearLayout>

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dp_12"
                    android:layout_marginTop="@dimen/dp_18"
                    android:layout_marginRight="@dimen/dp_12"
                    android:background="@color/bgColor10">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <TextView
                                android:id="@+id/preliminaryDiagnosisTitle"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/dp_18"
                                android:layout_marginTop="@dimen/dp_18"
                                android:text="@string/preliminary_diagnosis"
                                android:textColor="@color/textColor11"
                                android:textSize="@dimen/font_14" />


                            <TextView
                                android:id="@+id/preliminaryDiagnosisTv"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/dp_18"
                                android:layout_marginTop="@dimen/dp_18"
                                android:layout_marginRight="@dimen/dp_18"
                                android:layout_toRightOf="@+id/preliminaryDiagnosisTitle"
                                android:text=""

                                android:textColor="@color/textColor7"
                                android:textSize="@dimen/font_14" />

                        </RelativeLayout>


                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/dp_25"
                            android:layout_marginBottom="@dimen/dp_18">

                            <TextView
                                android:id="@+id/diagnosisSuggestTitle"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/dp_18"
                                android:layout_marginTop="@dimen/dp_18"
                                android:text="@string/diagnosis_suggest"
                                android:textColor="@color/textColor11"
                                android:textSize="@dimen/font_14" />


                            <TextView
                                android:id="@+id/diagnosisSuggestTitleTv"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/dp_18"
                                android:layout_marginTop="@dimen/dp_18"
                                android:layout_marginRight="@dimen/dp_18"
                                android:layout_toRightOf="@+id/diagnosisSuggestTitle"
                                android:text=""
                                android:textColor="@color/textColor7"
                                android:textSize="@dimen/font_14" />

                        </RelativeLayout>


                    </LinearLayout>


                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/prescriptionWap"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dp_12"
                    android:layout_marginTop="@dimen/dp_18"
                    android:layout_marginRight="@dimen/dp_12"
                    android:layout_marginBottom="@dimen/dp_18"
                    android:background="@color/bgColor10">

                    <LinearLayout

                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:focusable="true"
                            android:focusableInTouchMode="true">

                            <TextView
                                android:id="@+id/prescriptionTitle"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentLeft="true"
                                android:layout_centerInParent="true"
                                android:layout_marginLeft="@dimen/dp_18"
                                android:layout_marginTop="@dimen/dp_18"
                                android:layout_marginBottom="@dimen/dp_18"
                                android:text="@string/prescription"
                                android:textColor="@color/textColor7"
                                android:textSize="@dimen/font_14" />

                            <TextView
                                android:id="@+id/prescriptionTv"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerInParent="true"
                                android:layout_marginLeft="@dimen/dp_18"
                                android:layout_toRightOf="@+id/prescriptionTitle"
                                android:text="@string/prescription_tips"
                                android:textColor="@color/textColor16"
                                android:textSize="@dimen/font_12" />
                        </RelativeLayout>

                        <androidx.recyclerview.widget.RecyclerView
                            android:id="@+id/recyclerView"
                            android:layout_width="match_parent"
                            android:layout_height="1000dp" />

                    </LinearLayout>

                </androidx.cardview.widget.CardView>
            </LinearLayout>
        </ScrollView>

        <it.swiftelink.com.common.widget.stateview.StateView
            android:id="@+id/stateView"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />


    </FrameLayout>
</LinearLayout>